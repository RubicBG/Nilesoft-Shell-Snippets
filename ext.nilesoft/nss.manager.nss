			item(title=path.remove_extension(tm_nss[07]) tip=io.file.read(path.combine(tm_dir, tm_nss[07])) where=len(tm_nss)>07 vis=if(str.equals(tm_nss[07], main_file), 2) col=07%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[07]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[08]) tip=io.file.read(path.combine(tm_dir, tm_nss[08])) where=len(tm_nss)>08 vis=if(str.equals(tm_nss[08], main_file), 2) col=08%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[08]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[09]) tip=io.file.read(path.combine(tm_dir, tm_nss[09])) where=len(tm_nss)>09 vis=if(str.equals(tm_nss[09], main_file), 2) col=09%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[09]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[10]) tip=io.file.read(path.combine(tm_dir, tm_nss[10])) where=len(tm_nss)>10 vis=if(str.equals(tm_nss[10], main_file), 2) col=10%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[10]) pwt = main_path } & cmd_f)
