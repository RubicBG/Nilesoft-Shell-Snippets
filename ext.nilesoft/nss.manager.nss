			menu(title='font name' sep=1 image=\uE291 tip="Menu text font family\n\n"+"Sets the typeface used for all menu text. Font must be installed on the system") {
				$par='theme.font.name' // int; range: 0-10
				$get=ini.get(main_path, ini_group, par)
				$sup=1
				item(title='reset'
					image=[[\uE025],[\uE002]]
					vis=if(str.null(get), 2)
					cmd=cmd_s & ini.set(main_path, ini_group, par, null) & cmd_e)
				item(title='manual' tip=get sep=2 vis=0
					image=if(sup, [\uE179, image.color2], [\uE25B, #ff0000])
					cmd=if(input(msg_title, 'give valid @par value', get),
						cmd_s &	ini.set(main_path, ini_group, par, "'"+input.result+"'") & cmd_e))
				item(title='custom' keys='SHIFT more fonts' sep=1 tip='Use a font installed in the system. Hold SHIFT to see fonts with Bold/Italic variants' + if(len(get)>0, "\n\n" + get)
					cmd={
						$reg_fonts=reg.values('HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Fonts')
						$fonts_all=get
						for(i=0, i<len(reg_fonts)) {
							font=str.replace(reg_fonts[i], ' (TrueType)', null).replace(' (All res)', null)
							// too long names are not supported
							if(len(font)>30, continue)
							// skip bold/italic variants to reduce list size
							if(!key.shift() and (str.contains(font, 'Bold') or str.contains(font, 'Italic')), continue)
							// skip some bad fonts
							if(str.start(font, 'Wingdings') or str.start(font, 'Courier') or str.start(font, 'MS Serif') or str.start(font, 'MS Sans Serif'), continue)
							// skip some asian fonts
							if(str.contains('|Verdana|MS Gothic|MS Reference Specialty|Malgun Gothic SemiLight', '|'+font), continue)
							// build list
							fonts_all+="\n" + font }
						// msg(len(str.split(fonts_all, "\n")) + ' from ' + len(reg_fonts))
						if(input(msg_title, 'give valid theme.font.name value', get, str.split(fonts_all, "\n"), 0),
						cmd_s &	ini.set(main_path, ini_group, 'theme.font.name', "'"+input.result+"'") & cmd_e) }) }
