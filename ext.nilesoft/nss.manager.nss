			menu(title='success tip@"\t"array' image=svg_color tip=["Success tooltip color scheme\n\n"+"Sets background and text colors for success or confirmation tooltips", tip.success]) {
				$par=if(!str.null(ini.get(main_path, ini_group, 'settings.tip.success')), 'settings.tip.success', 'theme.tip.success')
				$get=ini.get(main_path, ini_group, par)
				$arr=regex.matches(get, '([^,\[\]\s]+(?:\([^)]*\))?)')
				$get_1=if(len(arr)<=3 and arr[0]!=null, arr[0], 'auto')
				$get_2=if(len(arr)<=3 and arr[1]!=null, arr[1], 'auto')
				$sup=str.null(get) or regex.match(get, '\[\s*(?:(?:\w+|#[0-9A-Fa-f]{6})(?:\s*,\s*(?:\w+|#[0-9A-Fa-f]{6}))*\s*)?\]')
