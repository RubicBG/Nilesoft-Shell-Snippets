			menu(title='information tip@"\t"array' image=svg_color tip=["Information tooltip color scheme\n\n"+"Sets background and text colors for informational message tooltips", tip.info]) {
				$par=if(!str.null(ini.get(main_path, ini_group, 'settings.tip.info')), 'settings.tip.info', 'theme.tip.info')
