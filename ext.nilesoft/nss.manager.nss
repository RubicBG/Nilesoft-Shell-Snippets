// global variables that can be edited
$theme_path_file = path.combine(path.parent(app.cfg), 'themes', 'default.nss')
$theme_menu_title = 'Shell &Manager'

// can change the type= pos= image= and where= values; do not change vis=
menu(title=theme_menu_title type='taskbar|desktop' vis=if(this.level!=1, 'disabled') image=[\uE249, image.color2]) {
	// variables that the user can change
		$mode_modern = 1
		$expand_thme = 1
		$expand_sttg = 1
		$ini_group = 'Config'
		$debug_msg =  key.rbutton()
	// auto generated variables
		// theme directory
		$tm_dir = eval(path.parent(theme_path_file))
		// themes file - auto generated
		$tm_nss = null
		// main theme file name
		$main_file = eval(if(mode_modern==0, path.title(theme_path_file)+'.nsi', path.name(theme_path_file)))
		$main_file_import = eval(path.name(theme_path_file))
		// main theme file path
		$main_path = eval(path.combine(tm_dir, main_file))
		$main_path_import = eval(path.combine(tm_dir, main_file_import))
		// message title
		$msg_title = eval(if(len(theme_menu_title)>5, theme_menu_title, 'Shell Manager'))
		// path-read-from
		$prf = null
		// path-write-to
		$pwt = null
	// basic complex commands
		$cmd_convert = {
			$go=1
			$debug=debug_msg
			// limit
			$limit = 400

			// Step 1: get all locations

			// if the path-read-from is not set, browse for it, Check if the file is found, ...
			if(go and prf==null, prf=path.file.box('nss|*.nss|all|*.*|', tm_dir))
			if(go and prf==null or not(path.exists(prf)) or path.isdir(prf), msg('The file to process was not found. Aborting', msg_title, msg.error) & {$go=0} )
			// if the path-write-to is not set, browse for location, ask for name. ...
			if(go and pwt==null, pwt=path.dir.box())
			if(go and pwt==null, { pwt=tm_dir} )
			if(go and path.isdir(pwt), input(msg_title, 'Enter a theme name') & { pwt=path.combine(pwt, if(len(input.result)==0, str.guid, input.result)+'.nss') } )
			// set the path-shell.nss
			$psh = app.cfg()
			if(debug, msg('Paths detected:'+"\n\n"+prf+"\n\n"+pwt+"\n\n"+psh))

			// Step 2: read files, extract data

			// read prf
			$prf_f = io.file.read(prf)
			$prf_f = regex.replace(regex.replace(regex.replace(prf_f, '\[\s*\n\s*', '[ '), '\s*\n\s*\]', ' ]'), ',\s*\n\s*', ', ')
			$lines = str.split(regex.replace(prf_f, "\n{2,}", "\n"), "\n")
			$stack = null
			$prf_s = null
			$prf_t = null
			$prf_r = null
			if(go, for(i=0, i<len(lines)) {
				// break if too many lines
				if(i==0 and len(lines)>limit, msg('Too many lines to process', msg_title, msg.error) and {$go=0})
				if(not(go), break)
				// get the current line
				line = lines[i]
				// ignore comments, [@ini_group], variables
				if(line().start('/'+'/') or line().start('/'+'*') or line().end('*'+'/') or line().contains('[@ini_group]') or line().start('$'), continue)
				// get the rest on the code
				$prf_r += if(stack().null and !line().start('theme') and !line().start('settings'), line() + "\n")
				// if the line contains a closing bracket, remove the last stack
				$stack = if(line().end('}'), str.left(stack, 1+str.findlast(str.trimend(stack, '.'), '.')), stack)
				// if the line contains an opening bracket, add a stack
				$stack += if(line().equals('{'), '.', if(line().start('{'), '.') + if(not(line().contains('=')), line().trim('{').trim()) + if(line().end('{'), '.')).trim('{}').trim()
				// if the stack starts with theme or settings, use it, otherwise nullify it
				$stack = if(stack().start('theme') or stack().start('settings'), stack, null)
				// if the line contains an equal sign and the stack starts with theme or settings, add it to the result
				$prf_t += if(line().contains('=') and (stack().start('theme') or line().start('theme')), stack + line().trim('{}').trim() + "\n")
				$prf_s += if(line().contains('=') and (stack().start('settings') or line().start('settings')), stack + line().trim('{}').trim() + "\n")
			})
			if(debug, msg('path-read-from' + "\n\n" + if(len(prf_t)>0, prf_t + "\n======\n") + if(len(prf_s)>0, prf_s + "\n======\n") + prf_r))
			if(len(prf_t)==0 and len(prf_s)==0, msg('No data to process', msg_title, msg.error) & {$go=0} )

			// check pwt
			$pwt_f = io.file.read(pwt)
			$pwt_f = regex.replace(regex.replace(regex.replace(pwt_f, '\[\s*\n\s*', '[ '), '\s*\n\s*\]', ' ]'), ',\s*\n\s*', ', ')
			$lines = str.split(regex.replace(pwt_f, "\n{2,}", "\n"), "\n")
			$stack = null
			$pwt_s = null
			$pwt_t = null
			$pwt_r = null
			if(go and path.exists(pwt), for(i=0, i<len(lines)) {
				if(i==0 and len(lines)>limit, msg('Too many lines to process', msg_title, msg.error) and {$go=0})
				if(not(go), break)
				line = lines[i]
				if(line().start('/'+'/') or line().start('/'+'*') or line().end('*'+'/') or line().contains('[@ini_group]')  or line().start('$'), continue)
				$pwt_r += if(stack().null and !line().start('theme') and !line().start('settings'), line() + "\n")
				$stack = if(line().end('}'), str.left(stack, 1+str.findlast(str.trimend(stack, '.'), '.')), stack)
				$stack += if(line().equals('{'), '.', if(line().start('{'), '.') + if(not(line().contains('=')), line().trim('{').trim()) + if(line().end('{'), '.')).trim('{}').trim()
				$stack = if(stack().start('theme') or stack().start('settings'), stack, null)
				$pwt_t += if(line().contains('=') and (stack().start('theme') or line().start('theme')), stack + line().trim('{}').trim() + "\n")
				$pwt_s += if(line().contains('=') and (stack().start('settings') or line().start('settings')), stack + line().trim('{}').trim() + "\n")
			})
			if(debug, msg('path-write-to' + "\n\n" + if(len(pwt_t)>0, pwt_t + "\n======\n") + if(len(pwt_s)>0, pwt_s + "\n======\n") + pwt_r))

			// check psh (path-shell.nss)
			$psh_f = io.file.read(psh)
			$psh_f = regex.replace(regex.replace(regex.replace(psh_f, '\[\s*\n\s*', '[ '), '\s*\n\s*\]', ' ]'), ',\s*\n\s*', ', ')
			$lines = str.split(regex.replace(psh_f, "\n{2,}", "\n"), "\n")
			$stack = null
			$psh_s = null
			$psh_t = null
			$psh_r = null
			if(go, for(i=0, i<len(lines)) {
				if(i==0 and len(lines)>limit, msg('Too many lines to process', msg_title, msg.error) and {$go=0})
				if(not(go), break)
				line = lines[i]
				if(line().start('/'+'/') or line().start('/'+'*') or line().end('*'+'/') or line().contains('[@ini_group]') or line().start('$'), continue)
				// skip some imports
				$psh_r += if(stack().null and !line().start('theme') and !line().start('settings'), line() + "\n")
				$stack = if(line().end('}'), str.left(stack, 1+str.findlast(str.trimend(stack, '.'), '.')), stack)
				$stack += if(line().equals('{'), '.', if(line().start('{'), '.') + if(not(line().contains('=')), line().trim('{').trim()) + if(line().end('{'), '.')).trim('{}').trim()
				$stack = if(stack().start('theme') or stack().start('settings'), stack, null)
				$psh_t += if(line().contains('=') and (stack().start('theme') or line().start('theme')), stack + line().trim('{}').trim() + "\n")
				$psh_s += if(line().contains('=') and (stack().start('settings') or line().start('settings')), stack + line().trim('{}').trim() + "\n")
			})
			if(debug, msg('path-shell.nss' + "\n\n" + if(len(psh_t)>0, psh_t + "\n======\n") + if(len(psh_s)>0, psh_s + "\n======\n") + psh_r))

			// Step 3: preparation

			// overwrite 'settings' with psh if needed
			$psh_m = psh_f
			$use_psh_s = pwt==main_path and len(psh_s)>0
			$use_psh_t = pwt==main_path and len(psh_t)>0
			if(go and pwt==main_path and (len(psh_s)>0 or len(psh_t)>0), {
				if(len(psh_s)>0, msg('"settings" have been detected in the "@path.name(app.cfg)" file. To ensure smooth operation, all "settings" will be transferred to the "@(path.name(tm_dir))/@main_file" file.'
					+ if(len(prf_s)>0, "\n\nNote: All settings from the source file will be skipped.")
					+ if(len(pwt_s)>0, "\n\nWarning: All previous settings will be overwritten.")
					+ "\n\n"+ psh_s+psh_t
					, msg_title, msg.warning))
				if(len(psh_t)>0, msg('"theme" have been detected in the "@path.name(app.cfg)" file. To ensure smooth operation, all "theme" will be removed.' +"\n\n"+ psh_t, msg_title, msg.warning))
				$psh_m = regex.replace(psh_f, `.*imports[\\/]+theme\.nss.*`, `// @msg_title: removed default theme import`)
				$psh_m = regex.replace(psh_m, `.*@(path.name(tm_dir))[\\/]+@main_file)\..*`, `// @msg_title: removed conflicting import`)
				$psh_m = regex.replace(psh_m, `.*themes[\\/]+default\..*`, `// @msg_title: removed old theme import`)
				$psh_m = regex.replace(psh_m, '(?:settings|theme)\s*\{[^{}]*?(?:\{[^{}]*\}[^{}]*?)*\}', '// @msg_title: removed settings' )
				$psh_m = regex.replace(psh_m, '(?:settings|theme)\.[^\n=]*\s*=\s*.*', '// @msg_title: removed settings' )
				})
			if(psh_f!=psh_m, msg('changes detected in path-shell.nss', msg.warning))
			prf_s = if(use_psh_s, psh_s, prf_s)

			// overwrite 'settings' with prf if needed
			$use_prf_s = go and !use_psh_s
				and if(not(prf==main_path and pwt==main_path) and len(prf_s)>0,
					msg('Settings have been detected in the source file. Do you want to use them? If not, they will be skipped.', msg_title, if(len(prf_t)>0, msg.defbutton2, msg.defbutton1)|msg.yesno)==msg.idyes, 1)
				and if(len(prf_s)==0 and len(pwt_s)>0,
					msg('Settings have been detected in the destination file. Do you want to keep them?', msg_title, msg.question|msg.yesno)==msg.idno, 1)
			pwt_s = if(use_prf_s, prf_s, pwt_s)

			// Step 4: create, backup, save, fix shell.nss
			// create folder
			// if(!path.exists(tm_dir), exec('cmd.exe', '/c md "@tm_dir"', null, null, 0, 1))
			if(!path.exists(tm_dir), io.dir.create(tm_dir))

			// fix theme.nss
			if(debug, msg('result' + "\n\n" + if(len(prf_t)>0, prf_t + "\n======\n") + if(len(pwt_s)>0, pwt_s + "\n======\n") + prf_r))
			pwt_f = if(pwt==main_path, "/*\n["+ini_group+"]\n*/\n\n")+pwt_s+prf_t+if(len(prf_r)>0, "\n/*\n"+prf_r+"*/")
			if(debug, msg(pwt_f))

			$pwt_old = path.rename_extension(pwt, ".old")
			if(go and pwt==main_path, io.delete(pwt_old) & io.copy(pwt, pwt_old))
			if(go and pwt==main_path, io.delete(pwt)) & io.file.create(pwt, pwt_f)

			$psh_old = path.rename_extension(psh, ".old")
			if(go and psh_f!=psh_m, io.delete(psh_old) & io.copy(psh, psh_old))
			if(go and psh_f!=psh_m, io.delete(psh) & io.file.create(psh, psh_m))

			// Step 5: nullify variables
			prf = null
			pwt = null }
		$cmd_two_c_empty = {
			if(!path.exists(tm_dir), io.dir.create(tm_dir))
			if(!path.exists(main_path), io.file.create(main_path, "\n["+ini_group+"]\n")) }
		$cmd_two_convert = io.file.create(main_path_import, str.trim(str.sub(str.replace(str.replace(io.file.read(main_path), ' ', ''), '`', ' '), str.len(ini_group)+2)))
	// theme commands
		$cmd_f = cmd_convert & app.reload & msg.beep
		$cmd_s = mode_modern ? { prf=main_path pwt=main_path } & cmd_convert : cmd_two_c_empty
		$cmd_e = mode_modern ? app.reload & msg.beep : cmd_two_convert & app.reload & msg.beep
	menu(where=mode_modern==1 expanded=true) {
		menu(title='@{tm_nss = if(tm_nss==null, path.files(tm_dir, '*.nss'), tm_nss)}Templates' image=if(path.exists(main_path), \uE117, \uE116)) {
			item(title='empty' tip='import template or create an empty one' where=len(tm_nss)==0 vis=2)
			$rows = 28
			item(title=path.remove_extension(tm_nss[00]) tip=io.file.read(path.combine(tm_dir, tm_nss[00])) where=len(tm_nss)>00 vis=if(str.equals(tm_nss[00], main_file), 2) col=00%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[00]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[01]) tip=io.file.read(path.combine(tm_dir, tm_nss[01])) where=len(tm_nss)>01 vis=if(str.equals(tm_nss[01], main_file), 2) col=01%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[01]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[02]) tip=io.file.read(path.combine(tm_dir, tm_nss[02])) where=len(tm_nss)>02 vis=if(str.equals(tm_nss[02], main_file), 2) col=02%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[02]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[03]) tip=io.file.read(path.combine(tm_dir, tm_nss[03])) where=len(tm_nss)>03 vis=if(str.equals(tm_nss[03], main_file), 2) col=03%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[03]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[04]) tip=io.file.read(path.combine(tm_dir, tm_nss[04])) where=len(tm_nss)>04 vis=if(str.equals(tm_nss[04], main_file), 2) col=04%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[04]) pwt = main_path } & cmd_f)
			item(title=path.remove_extension(tm_nss[05]) tip=io.file.read(path.combine(tm_dir, tm_nss[05])) where=len(tm_nss)>05 vis=if(str.equals(tm_nss[05], main_file), 2) col=05%rows==0 cmd={ prf = path.combine(tm_dir, tm_nss[05]) pwt = main_path } & cmd_f)
